{"ast":null,"code":"import { watch, watchEffect } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport usePokemon from \"../composables/usePokemon\";\nexport default {\n  setup() {\n    const route = useRoute();\n    const {\n      pokemon,\n      isLoading,\n      errorMessage\n    } = usePokemon(route.params.id);\n    watchEffect(console.log(route.params.id));\n    return {\n      pokemon,\n      isLoading,\n      errorMessage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAaA,SAASA,KAAT,EAAgBC,WAAhB,QAAmC,KAAnC;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,OAAOC,UAAP,MAAuB,2BAAvB;AAEA,eAAe;AACbC,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AAEA,UAAM;AAAEI,aAAF;AAAWC,eAAX;AAAsBC;AAAtB,QAAuCL,UAAU,CAACE,KAAK,CAACI,MAAN,CAAaC,EAAd,CAAvD;AACAT,eAAW,CAACU,OAAO,CAACC,GAAR,CAAYP,KAAK,CAACI,MAAN,CAAaC,EAAzB,CAAD,CAAX;AAGA,WAAO;AACLJ,aADK;AAELC,eAFK;AAGLC;AAHK,KAAP;AAKD;;AAbY,CAAf","names":["watch","watchEffect","useRoute","usePokemon","setup","route","pokemon","isLoading","errorMessage","params","id","console","log"],"sourceRoot":"","sources":["/home/walter/Escritorio/vue/08-compo-api/src/views/Pokemon.vue"],"sourcesContent":["<template>\n  <h1 v-if=\"!pokemon && !errorMessage\">Loading...</h1>\n  <h1 v-else-if=\"errorMessage\">{{ errorMessage }}</h1>\n\n  <div v-else>\n    <h3>{{ pokemon.name }}</h3>\n    <img :src=\"pokemon.sprites.front_default\" :alt=\"pokemon.name\" />\n    <br />\n    <router-link :to=\"{ name: 'search' }\">Search</router-link>\n  </div>\n</template>\n\n<script>\nimport { watch, watchEffect } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport usePokemon from \"../composables/usePokemon\";\n\nexport default {\n  setup() {\n    const route = useRoute();\n\n    const { pokemon, isLoading, errorMessage } = usePokemon(route.params.id);\n    watchEffect(console.log(route.params.id))\n\n\n    return {\n      pokemon,\n      isLoading,\n      errorMessage,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module